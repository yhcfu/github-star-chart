{
  "name": "github-star-chart",
  "version": "1.0.0",
  "description": "",
  "homepage": "https://yhcfu.github.io/github-star-chart/",
  "license": "MIT",
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "_gen:schema": "gql.tada generate-schema $GITHUB_GRAPHQL_URL --header \"Authorization: bearer $GITHUB_TOKEN\"",
    "build": "react-router build",
    "check": "biome check --write ./src",
    "dev": "react-router dev",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "gen:schema": "dotenv -- pnpm _gen:schema",
    "postinstall": "lefthook install",
    "nuke": "pnpm dlx rimraf node_modules && pnpm i",
    "preview": "vite preview",
    "typecheck": "react-router typegen && tsc --noEmit"
  },
  "dependencies": {
    "@radix-ui/react-avatar": "^1.1.2",
    "@radix-ui/react-dialog": "^1.1.4",
    "@radix-ui/react-dropdown-menu": "^2.1.4",
    "@radix-ui/react-slot": "^1.1.1",
    "@react-router/fs-routes": "^7.0.2",
    "@react-router/node": "^7.0.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "1.0.0",
    "es-toolkit": "^1.30.1",
    "gql.tada": "^1.8.10",
    "isbot": "^5.1.18",
    "lucide-react": "^0.468.0",
    "nuqs": "^2.2.3",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-error-boundary": "^4.1.2",
    "react-resizable-panels": "^2.1.7",
    "react-router": "^7.0.2",
    "react-twc": "^1.4.2",
    "recharts": "^2.15.0",
    "safe-routes": "^1.0.2",
    "tailwind-merge": "^2.5.5",
    "tailwindcss-animate": "^1.0.7",
    "urql": "^4.2.1",
    "zustand": "^5.0.2"
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "@react-router/dev": "^7.0.2",
    "@tailwindcss/typography": "^0.5.15",
    "@types/node": "^22.10.2",
    "@types/react": "^18.3.17",
    "@types/react-dom": "^18.3.5",
    "autoprefixer": "^10.4.20",
    "dotenv-cli": "^7.4.4",
    "lefthook": "^1.9.2",
    "postcss": "^8.4.49",
    "prettier": "^3.4.2",
    "prettier-plugin-classnames": "^0.7.5",
    "prettier-plugin-packagejson": "^2.5.6",
    "tailwindcss": "^3.4.16",
    "turbo": "^2.3.3",
    "typescript": "^5.7.2",
    "vite": "^5.4.11",
    "vite-plugin-svgr": "^4.3.0",
    "vite-tsconfig-paths": "^5.1.4"
  },
  "engines": {
    "node": ">=22.0.0"
  }
